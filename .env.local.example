# SMTP settings for sending form submissions (Get a Quote / Book a Session)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=

# Optional: override where form submissions are delivered (defaults to the email shown on the Contact page)
# CONTACT_EMAIL=equalizerjr@gmail.com
CONTACT_EMAIL=

# Optional: override the From address (defaults to SMTP_USER)
SMTP_FROM=

# SEO / Site settings
# Set this to your real production domain once you have it (keep the http/https)
NEXT_PUBLIC_SITE_URL=

# Google Search Console verification (HTML tag method)
# Example: <meta name="google-site-verification" content="..." />
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=
